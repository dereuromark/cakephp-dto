{% if collectionType and singular %}
{% set cleanSingular = singular | stripLeadingUnderscore %}
	/**
{% if deprecated is not null %}
	 * @deprecated {{ deprecated }}
	 *
{% endif %}
	 * @param {{ singularType }}{% if singularNullable %}|null{% endif %} ${{ singular }}
	 *
	 * @return $this
	 */
	public function add{{ cleanSingular[:1]|upper ~ cleanSingular[1:] }}({% if singularTypeHint %}{% if singularNullable %}?{% endif %}{{ singularTypeHint }} {% endif %}${{ singular }}) {
		if ($this->{{ name }} === null) {
			$this->{{ name }} = new {{ typeHint }}([]);
		}

		$this->{{ name }} = $this->{{ name }}->appendItem(${{ singular }});
		$this->_touchedFields[static::FIELD_{{ name | stripLeadingUnderscore | underscore | upper }}] = true;

		return $this;
	}
{% endif -%}
