{% if collection %}
{% set cleanSingular = singular | stripLeadingUnderscore %}
{% set adapter = getCollectionAdapter(collectionType) %}
	/**
{% if deprecated is not null %}
	 * @deprecated {{ deprecated }}
	 *
{% endif %}
{% if associative %}
	 * @param {{ keyType }} $key
{% endif %}
	 * @param {{ singularType }}{% if singularNullable %}|null{% endif %} ${{ singular }}
	 * @return static
	 */
	public function withAdded{{ cleanSingular[:1]|upper ~ cleanSingular[1:] }}({% if associative %}$key, {% endif %}{% if singularTypeHint %}{% if singularNullable %}?{% endif %}{{ singularTypeHint }} {% endif %}${{ singular }}) {
		$new = clone $this;

		if ($new->{{ name }} === null) {
			$new->{{ name }} = {% if collectionType == 'array' %}[]{% else %}{{ adapter.createEmptyCode(typeHint) }}{% endif %};
		}

{% if associative %}
		$new->{{ name }}[$key] = ${{ singular }};
{% elseif collectionType == 'array' %}
		$new->{{ name }}[] = ${{ singular }};
{% elseif adapter.immutable %}
		$new->{{ name }} = $new->{{ name }}->{{ adapter.appendMethod }}(${{ singular }});
{% else %}
		$new->{{ name }}->{{ adapter.appendMethod }}(${{ singular }});
{% endif %}
		$new->_touchedFields[static::FIELD_{{ name | stripLeadingUnderscore | underscore | upper }}] = true;

		return $new;
	}
{% endif -%}
