	/**
	* @var array
	*/
	protected $_keyMap = [
		'underscored' => [
{% for field in fields %}
			'{{ field.name | underscore }}' => '{{ field.name }}',
{% endfor %}
		],
		'dashed' => [
{% for field in fields %}
			'{{ field.name | underscore | replace({'_': "-"}) }}' => '{{ field.name }}',
{% endfor %}
		],
	];
