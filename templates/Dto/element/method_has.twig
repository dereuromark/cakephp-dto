{% set cleanName = name | stripLeadingUnderscore %}
	/**
{% if deprecated is not null %}
	 * @deprecated {{ deprecated }}
	 *
{% endif %}
	 * @return bool
	 */
	public function has{{ cleanName[:1]|upper ~ cleanName[1:] }}(){% if scalarAndReturnTypes %}: bool{% endif %} {
{% if collectionType %}
		if ($this->{{ name }} === null) {
			return false;
		}

{% if collectionType == 'array' %}
		return count($this->{{ name }}) > 0;
{% else %}
		return $this->{{ name }}->count() > 0;
{% endif %}
{% else %}
		return $this->{{ name }} !== null;
{% endif %}
	}
{%- if associative and singular and not singularNullable%}


	/**
{% if deprecated is not null %}
	 * @deprecated {{ deprecated }}
	 *
{% endif %}
	 * @param {{ keyType }} $key
	 * @return bool
	 */
{% set cleanSingular = singular | stripLeadingUnderscore %}
	public function has{{ cleanSingular[:1]|upper ~ cleanSingular[1:] }}({{ keyType }} $key){% if scalarAndReturnTypes %}: bool{% endif %} {
		return isset($this->{{ name }}[$key]);
	}
{% endif -%}
