{% set cleanName = name | stripLeadingUnderscore %}
	/**
{% if deprecated is not null %}
	 * @deprecated {{ deprecated }}
	 *
{% endif %}
	 * @throws \RuntimeException If value is not set.
	 *
	 * @return {{ docBlockType ?? type }}
	 */
	public function get{{ cleanName[:1]|upper ~ cleanName[1:] }}OrFail(){% if returnTypeHint %}: {{ returnTypeHint }}{% endif %} {
		if ($this->{{ name }} === null) {
			throw new \RuntimeException('Value not set for field `{{ name }}` (expected to be not null)');
		}

		return $this->{{ name }};
	}
